<!--管理者側User一覧画面-->
<div class="container">
  <h5>会員一覧</h5>
  <div class="row">
    <div class="col">
      <% @users.each do |user| %>
      <table class="table">
        <tbody>
          <tr>
            <td>会員ID</td>
            <td><%= user.id %></td>
          </tr>
          <tr>
            <td>ユーザー名</td>
            <td><%= user.account_name %></td>
          </tr>
          <tr>
            <td>メールアドレス</td>
            <td><%= user.email %></td>
          </tr>
          <tr>
            <td>会員ステータス</td>
            <td></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>