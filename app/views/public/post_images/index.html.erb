<div class="container">
  <h5>index</h5>
  <div class="row">
    <div class="col">
      <% @post_images.each do |post_image| %>
      <div>
        
          <%= post_image.user.name %>
  
        <%= link_to post_image_path(post_image.id) do %>
          <%= attachment_image_tag post_image, :image, :fill, 200,200 %>
        <% end %>
      </div>
      <% end %>
    </div>
  </div>
</div>